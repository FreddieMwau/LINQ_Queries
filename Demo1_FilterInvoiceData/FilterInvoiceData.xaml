    <Assign DisplayName="LINQ to filter out Overdue Invoices" sap:VirtualizedContainerService.HintSize="434,114" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[dt_FilteredInvoices]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable">[in_dt_Invoices.AsEnumerable.Where(Function (row) row("Total").ToString.Contains("USD") AndAlso Not row("Date").ToString.Contains("0000") AndAlso DateTime.ParseExact(row("Date").ToString, "yyyy-MM-dd", System.Globalization.CultureInfo.InvariantCulture) &lt; DateTime.ParseExact(str_GivenDate, "yyyy-MM-dd", System.Globalization.CultureInfo.InvariantCulture) ).CopyToDataTable]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:InvokeCode ContinueOnError="{x:Null}" sap2010:Annotation.AnnotationText="This will arrange all the invoices in descending order, to achieve this we'll eliminate the currency from the invoice total value" Code="dt_FilteredInvoices.AsEnumerable().ToList().ForEach(Sub (row) row(&quot;Total&quot;) = row(&quot;Total&quot;).ToString.Split(&quot; &quot;c).First)           " DisplayName="LINQ to Remove Currency from Total" sap:VirtualizedContainerService.HintSize="434,254" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
      <ui:InvokeCode.Arguments>
        <InArgument x:TypeArguments="sd:DataTable" x:Key="dt_FilteredInvoices">
